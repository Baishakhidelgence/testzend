        function save2(datam) {
           
            var name = $("#cname").val();
            var type = $("#ctype").val();
            var phone = $("#cphone").val();
            var city = $("#ccity").val();
            var country = $("#ccountry").val();
            var desc = $("#desc").val();
            var imgs = $("#devimage").attr("src");
            var basepath = window.location.protocol + "//" + window.location.host;
            if (imgs == "") {

                var imgs = basepath+"/img/default-profile.png";
            }
            if (  phone == "" || city == "" || country == "" || country == "Selected a country") {
                //alert("please provide required field");
                $("#validate").text("Please provide required field");
                return false;
            }
            if ((jQuery.trim(desc)).length == 0) {
                //alert("please provide Description");
                $("#validate").text("Please provide required field");
                return false;
            }

            $.ajax({
                type: 'POST',
                data: {
                    name: name,
                    type: type,
                    phone: phone,
                    city: city,
                    country: country,
                    desc: desc,
                    imgs: imgs
                },
                url: basepath + "/sfpdeveloper/updateReg/" + datam,
                success: function(data) {

                    window.location.href = basepath + "/sfpdeveloper/devreg2/" + datam;
                }
            });
        }
        

                    }
                }
            });
        }
        function enterkey(e, t) {
            if (window.event) {
                var charCode = window.event.keyCode;
            } else if (e) {
                var charCode = e.which;
            } else {
                return true;
            }

            if ((charCode == 13))
                continuelog();
            else
                return true;
        }

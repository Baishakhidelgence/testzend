<?php 
function curPageURL() 
{
    $pageURL = 'http';
    if (@$_SERVER["HTTPS"] == "on") {$pageURL .= "s";}
    $pageURL .= "://";
    if ($_SERVER["SERVER_PORT"] != "80") {
        if($_SERVER["REQUEST_URI"] == "/")
            $pageURL .= $_SERVER["SERVER_NAME"].":".$_SERVER["SERVER_PORT"].'/index/index';
        else
            $pageURL .= $_SERVER["SERVER_NAME"].":".$_SERVER["SERVER_PORT"].$_SERVER["REQUEST_URI"];
    } else {
        if($_SERVER["REQUEST_URI"] == "/")
            $pageURL .= $_SERVER["SERVER_NAME"].'/index/index';
        else
            $pageURL .= $_SERVER["SERVER_NAME"].$_SERVER["REQUEST_URI"];
    }
    return $pageURL;
}
$href = explode("/", curPageURL());
$page = @$href[4];
$pagefront = @$href[4];
